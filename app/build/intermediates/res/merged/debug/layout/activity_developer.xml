<?xml version="1.0" encoding="utf-8"?><LinearLayout    xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical">    <RelativeLayout        android:id="@+id/actionBar"        android:layout_width="match_parent"        android:layout_height="?attr/actionBarSize"        android:background="@color/actionBar">        <ImageView            android:id="@+id/bannerBack"            style="@style/BannerBackButton"            android:onClick="dismissActivity" />        <TextView            style="@style/BannerBackText"            android:layout_toRightOf="@id/bannerBack"            android:onClick="dismissActivity" />        <TextView            android:id="@+id/bannerTitle"            android:text="开发者选项"            style="@style/BannerTitle" />    </RelativeLayout>    <ListView        android:id="@+id/list_setting"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_alignParentLeft="true"        android:layout_alignParentStart="true"        android:paddingLeft="10dp"        android:divider="@drawable/list_divider_line"        android:dividerHeight="0.5dp"        android:layout_marginTop="5dp">    </ListView>    <View style="@style/Divider"/></LinearLayout>